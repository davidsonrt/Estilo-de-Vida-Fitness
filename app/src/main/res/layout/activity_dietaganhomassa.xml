                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="#771C1717"
                android:fontFamily="@font/chelsea_market"
                android:gravity="center"
                android:text="4 colheres de sopa de arroz integral 1 concha de feijão 2 filés de 100 g de frango grelhado 1/2 prato de verduras (cruas ou refogadas)"
