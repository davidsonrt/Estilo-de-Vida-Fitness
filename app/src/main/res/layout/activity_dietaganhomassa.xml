                android:textColorHighlight="#FFFFFF"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="#771C1717"
                android:fontFamily="@font/chelsea_market"
                android:gravity="center"
                android:text="Café da manhã"
                android:textColor="#FFFFFF"
                android:textSize="18dp" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:background="#771C1717"
                android:fontFamily="@font/chelsea_market"
                android:gravity="center"
                android:text="2 fatias de pão de forma integral 2 fatias de queijo minas frescal 1 copo grande 300 ml de leite desnatado com café 2 fatias de mamão com 1 colher de sopa de farelo de aveia"
